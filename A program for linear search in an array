#include <stdio.h>

int main()
{
    int n, num;
    printf("Enter the size of the array \n");
    scanf("%d", &n);
    printf("Enter %d elements for the array \n", n);
    int arr[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d", &arr[i]);
    }
    printf("Enter the element to be searched in the array \n");
    scanf("%d", &num);
    for( int j=0;j<n;j++)
    {
        if(arr[j]==num)
        {
            printf("Element found at location %d", j+1);
            return 0;
        }
    }
   
  printf("Element not found in the array \n");
  return 0;
}
